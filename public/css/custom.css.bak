/* settings page */

.btn-with-toggle input[type=checkbox]{
	height: 0;
	width: 0;
	visibility: hidden;
}

.btn-with-toggle label {
	cursor: pointer;
    text-indent: -9999px;
    width: 35px;
    height: 23px;
    background: #e0e0e0;
    display: inline-block;
    border-radius: 100px;
    position: relative;
    margin: 0;
}

.btn-with-toggle label:after {
	content: '';
    position: absolute;
    top: 4px;
    left: 4px;
    width: 15px;
    height: 15px;
    background: #9c9c9c;
    border-radius: 50%;
    transition: 0.3s;
}

.btn-with-toggle input:checked + label {
	background: #d0f6d6;
}

.btn-with-toggle input:checked + label:after {
	background: #00d000;
}

.btn-with-toggle input:checked + label:after {
	left: calc(100% - 4px);
    transform: translateX(-100%);
}

.btn-with-toggle label:active:after {
	width: 22px;
}

