.register-checkbox input{
	display: none;
    visibility: hidden;
    opacity: 0;
}

.register-checkbox label{
    user-select: none;
    margin: 22px 0 0 25px;
    position: relative;
}

.register-checkbox label::before {
	content: "\f00c";
    font-family: "Font Awesome 5 Free";
    position: absolute;
    font-weight: 900;
    width: 18px;
    height: 18px;
    top: 4px;
    margin-left: -23px;
cursor: pointer;
    font-size: 13px;
    line-height: 1.4;
    border: 1px solid #ececec;
    border-radius: 3px;
    text-align: center;
    color: transparent;
    -webkit-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;
    -o-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;
    transition: border 0.15s ease-in-out, color 0.15s ease-in-out;
}

.register-checkbox input[type="checkbox"]:checked+label::before {
    color: #fff;
    background-color: rgb(17, 106, 184);
}

.login-checkbox input[type="checkbox"]:checked+label::before {
    color: #fff;
    background-color: rgb(35, 194, 119);
}

.setting-checkbox label::before {
    width: 25px;
    height: 25px;
    line-height: 1.9;
}

.setting-checkbox input[type="checkbox"]:checked+label::before {
    color: #fff;
    background-color: #00cf00;
}
